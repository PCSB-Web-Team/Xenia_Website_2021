setRegisteredEvents, getEventDetails } from \"../../Config/api/User\";\r\nimport { addToCartFail, registrationFail, registrationSuccess } from \"../../Notifications/Notification\";\r\nimport Modal from \"./Modal/Modal\";\r\nimport Themebutton from \"../../Button/button\";\r\n\r\nconst MoreInfo = (props) => {\r\n  \r\n  const [details, setDetails] = useState();\r\n  const [loading, setLoading] = useState(true);\r\n  const [registered, setRegistered] = useState(false);\r\n  const [showModal, setShowModal] = useState(false);\r\n  const [registerLoading, setRegisterLoading] = useState(false);\r\n\r\n  let { id } = useParams();\r\n  let history = useHistory();\r\n\r\n  const checkRegistered = () => {\r\n    setRegistered(false);\r\n    props.registeredEvents.forEach((eve) => {\r\n      if (eve._id === id) setRegistered( true );\r\n    });\r\n  };\r\n\r\n  useEffect(() => {\r\n    fetchData();\r\n    checkRegistered();\r\n  }, [id]);\r\n\r\n  const fetchData = async (props) => {\r\n    try {\r\n      const response = await getEventDetails(id);\r\n\r\n      console.log(response.data);\r\n\r\n      if (response.data.ok) {\r\n        await setDetails(() => response.data.data);\r\n        console.log(details);\r\n      }\r\n    } catch (error) {\r\n      console.log(error);\r\n      history.push(\"/events\");\r\n    }\r\n\r\n    setLoading(false);\r\n  };\r\n\r\n  const openModal = () => {\r\n    setShowModal(true);\r\n    setRegisterLoading(false);\r\n  };\r\n  const closeModal = () => {\r\n    setShowModal(false);\r\n    setRegisterLoading(false);\r\n  };\r\n\r\n  const handleRegister = async () => {\r\n    setRegisterLoading(true);\r\n\r\n    try{\r\n\r\n    if (props.isLoggedIn) {\r\n      \r\n      // if(details.additionalInfo.required){\r\n      // showPopUp\r\n      // fire modal and recieve the details object\r\n      // }\r\n\r\n      const res = await setRegisteredEvents(id, props.token);\r\n\r\n      if (res.data.ok) {\r\n        props.addToRegistered(res.data.data.event);\r\n        // addToCartSuccess();\r\n        setRegistered(true);\r\n        registrationSuccess();\r\n        setRegisterLoading(false);\r\n        setShowModal(false);\r\n      }\r\n      else{\r\n        closeModal();\r\n        // registrationsClosed();\r\n      }\r\n    } else {\r\n      addToCartFail();\r\n    }\r\n  }\r\n  catch{\r\n    registrationFail();\r\n  }\r\n\r\n  };\r\n\r\n  return (\r\n    <div className=\"MoreInfo\">\r\n      {loading ? (\r\n        <Loader />\r\n      ) : (\r\n        <div className=\"more-info\">\r\n          <Link to=\"/events\">\r\n            <div className=\"back-container\">\r\n              <img src={back2} alt=\"go back\" />\r\n            </div>\r\n          </Link>\r\n\r\n          <div\r\n            className=\"jumbotron text-center py-2\"\r\n            id=\"main-detail\"\r\n          >\r\n            <img className=\"logo\" src={details.logo} alt=\"logo\"></img>\r\n\r\n            <h3 className=\"name\">{details.name}</h3>\r\n            {/* <span className> {details.date.substring(0, 10)} </span> */}\r\n\r\n            <p className=\"lead\">\r\n              {details.details}\r\n            </p>\r\n\r\n            <hr className=\"my-1\" />\r\n\r\n            {!registered ? (\r\n              <div\r\n                style={{\r\n                  display: \"flex\",\r\n                  flexFlow: \"column\",\r\n                  alignItems: \"center\",\r\n                }}\r\n              >\r\n                <Themebutton\r\n                  onClick={props.isLoggedIn ? openModal : props.openLogin}\r\n                  value=\"Register Now\"\r\n                />\r\n              </div>\r\n            ) : (\r\n              <span className='already-registered'>\r\n                Registered\r\n              </span>\r\n            )}\r\n\r\n            <DetailsTab details={details} />\r\n          </div>\r\n\r\n          <Suggestion suggestions={details.suggestions}></Suggestion>\r\n        </div>\r\n      )}\r\n      <Modal\r\n        showModal={showModal}\r\n        closeModal={closeModal}\r\n        openModal={openModal}\r\n        load={registerLoading}\r\n        handleRegister={handleRegister}\r\n      />\r\n    </div>\r\n  );\r\n};\r\n\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    token: state.token,\r\n    isLoggedIn: state.login,\r\n    registeredEvents: state.userData.registeredEvents,\r\n  };\r\n};\r\n\r\nconst mapActionToProps = (dispatch) => {\r\n  return {\r\n    openLogin: () => {\r\n      dispatch(openLogin());\r\n    },\r\n    addToRegistered: (details) => {\r\n      dispatch(addToRegistered(details));\r\n    },\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps, mapActionToProps)(MoreInfo);\r\n","/home/vedant/Projects/xenia-dashboard/src/Components/Contact/Contact.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Sponsors/Sponsors.js",[],["190","191"],"/home/vedant/Projects/xenia-dashboard/src/Components/AboutUs/About.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/WebTeam/WebTeam.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Footer/Footer.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Profile/Profile.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/BackGround/Background.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Config/api/User.js",[],"/home/vedant/Projects/xenia-dashboard/src/Store/DefaultState.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Config/config.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Events/Cards/Card.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/WebTeam/TeamProfile.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Loader/Loader.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Registrations/RegOpen/RegOpen.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Registrations/EventCols/EventCols.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Even