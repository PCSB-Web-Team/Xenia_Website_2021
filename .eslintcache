State(true);\r\n\r\n  useEffect(() => { fetchData() }, []);\r\n\r\n  const fetchData = async () => {\r\n    try {\r\n\r\n      const response = await getAllEvents();\r\n\r\n      if (response.data.ok) {\r\n\r\n        let tech = [];\r\n        let nonTech = [];\r\n\r\n        tech = response.data.data.filter(eve => eve.isTechnical)\r\n        nonTech = response.data.data.filter(eve => !eve.isTechnical)\r\n\r\n        setTech(tech);\r\n        setNonTech(nonTech);\r\n\r\n      }\r\n\r\n    }\r\n    catch (err) {\r\n      console.log(err)\r\n    }\r\n\r\n    setLoading(false);\r\n  }\r\n\r\n  const changeEventType = () => {\r\n    if (eventType === 'tech') setEventType('nonTech')\r\n    else setEventType('tech')\r\n  }\r\n\r\n  return (\r\n\r\n    <div className=\"events-dashboard\">\r\n\r\n      <header className='page-headers'><h1> EVENTS </h1></header>\r\n\r\n      {loading ? <Loader /> :\r\n\r\n        <div className=\"card-container\">\r\n\r\n          <div className=\"tabs\">\r\n            <button className={eventType === 'tech' ? 'tabs-btn active-tab' : 'tabs-btn'} onClick={() => setEventType('tech')} id='tech-tab'>Tech</button>\r\n            <button className={eventType !== 'tech' ? 'tabs-btn active-tab' : 'tabs-btn'} onClick={() => setEventType('non-tech')} id='non-tech-tab'>Non-Tech</button>\r\n          </div>\r\n\r\n          {eventType === 'tech' ? techEvents.map(eve => { return (<div key={eve._id} className='card-div'>  <Link to={`/events/${eve._id}`}><Card details={eve} ></Card></Link> </div>) }) : null}\r\n          {eventType !== 'tech' ? nonTechEvents.map(eve => { return (<div key={eve._id} className='card-div'>  <Link to={`/events/${eve._id}`}><Card details={eve} ></Card></Link> </div>) }) : null}\r\n\r\n        </div>\r\n\r\n      }\r\n\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Events;\r\n","/home/vedant/Projects/xenia-dashboard/src/Components/Navbar/Navbar.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Events/MoreInfo/MoreInfo.js",["215"],"import React, { useEffect, useState } from \"react\";\r\nimport \"./MoreInfo.css\";\r\nimport back2 from \"../../../Assets/Images/arrow-left2.png\";\r\nimport { connect } from \"react-redux\";\r\nimport { useParams, Link, useHistory } from \"react-router-dom\";\r\nimport ReactLogo from \"../../../Assets/Images/logo.svg\";\r\nimport DetailsTab from \"./DetailTabs/DetailsTabs\";\r\nimport Suggestion from \"./Suggestion/Suggestion\";\r\nimport { openLogin, addToRegistered } from \"../../../Store/Actions\";\r\nimport Loader from \"../../Loader/Loader\";\r\nimport { setRegisteredEvents, getEventDetails } from \"../../Config/api/User\";\r\nimport { addToCartFail } from '../../Notifications/Notification';\r\nimport Modal from './Modal/Modal';\r\n\r\nconst MoreInfo = (props) => {\r\n\r\n  const [details, setDetails] = useState({ date: '', rules: [] });\r\n  const [loading, setLoading] = useState(true);\r\n  const [registered, setRegistered] = useState(false);\r\n  const [showModal, setShowModal] = useState(false);\r\n  const [registerLoading, setRegisterLoading] = useState(false)\r\n\r\n  let { id } = useParams();\r\n  let history = useHistory();\r\n\r\n  const checkRegistered = () => {\r\n    setRegistered(false);\r\n    props.registeredEvents.forEach(eve => {\r\n      if (eve._id === id) setRegistered(true);\r\n    });\r\n  };\r\n\r\n  useEffect(() => {\r\n    fetchData();\r\n    checkRegistered();\r\n  }, [id]);\r\n\r\n  const fetchData = async (props) => {\r\n\r\n    try {\r\n      const response = await getEventDetails(id);\r\n\r\n      console.log(response.data)\r\n\r\n      if (response.data.ok) {\r\n        setDetails(response.data.data);\r\n      }\r\n    } catch (error) {\r\n      console.log(error)\r\n      history.push('/events');\r\n    }\r\n\r\n    setLoading(false);\r\n  };\r\n\r\n  \r\n  const openModal = () => {setShowModal(true); setRegisterLoading(false)};\r\n  const closeModal = () => {setShowModal(false); setRegisterLoading(false)};\r\n\r\n  const handleRegister = async () => {\r\n\r\n    setRegisterLoading(true)\r\n\r\n    if (props.isLoggedIn) {\r\n\r\n      // if(details.additionalInfo.required){\r\n      // showPopUp\r\n      // fire modal and recieve the details object\r\n      // }\r\n\r\n      const res = await setRegisteredEvents(id, props.token);\r\n\r\n      if (res.data.ok) {\r\n        props.addToRegistered(res.data.data.event);\r\n        // addToCartSuccess();\r\n        setRegistered(true);\r\n        setRegisterLoading(false)\r\n        setShowModal(false);\r\n      }\r\n    } else {\r\n      addToCartFail();\r\n    }\r\n\r\n    \r\n  };\r\n\r\n\r\n  return (\r\n    <div className=\"MoreInfo\">\r\n      {loading ? (\r\n        <Loader />\r\n      ) : (\r\n        <div className=\"info1\">\r\n\r\n          <Link to=\"/events\">\r\n            <div className=\"back-container\">\r\n              <img src={back2} alt='go back' />\r\n            </div>\r\n          </Link>\r\n\r\n          <div className=\"more-info jumbotron text-center py-2\" id=\"main-detail\">\r\n\r\n            <img className=\"logo\" src={ReactLogo} alt='logo'></img>\r\n\r\n            <h3 className=\"name\">{details.name}</h3>\r\n            <span className> {details.date.substring(0, 10)} </span>\r\n\r\n            <p className=\"lead\">\r\n              This is a simple hero unit, a simple jumbotron-style component for\r\n              calling extra attention to featured content or information.\r\n            </p>\r\n\r\n            <hr className=\"my-1\" />\r\n\r\n            {!registered ? (\r\n              <div\r\n                onClick={props.isLoggedIn ? openModal : props.openLogin}\r\n                className=\"btn btn-lg bg-success\"\r\n                role=\"button\"\r\n              >\r\n                Register Now\r\n              </div>\r\n            )\r\n              : <span style={{ color: 'blue', fontWeight: 'bold', fontSize: '20px' }}>Registered</span>\r\n            }\r\n\r\n            <DetailsTab details={details} />\r\n          </div>\r\n\r\n          <Suggestion suggestions={details.suggestions}></Suggestion>\r\n        </div>\r\n      )}\r\n      <Modal showModal={showModal} closeModal={closeModal} openModal={openModal} load={registerLoading} handleRegister={handleRegister}/>\r\n    </div>\r\n  );\r\n};\r\n\r\nconst mapStateToProps = (state) => {\r\n  return {\r\n    token: state.token,\r\n    isLoggedIn: state.login,\r\n    registeredEvents: state.userData.registeredEvents\r\n  };\r\n};\r\n\r\nconst mapActionToProps = (dispatch) => {\r\n  return {\r\n    openLogin: () => {\r\n      dispatch(openLogin());\r\n    },\r\n    addToRegistered: details => {\r\n      dispatch(addToRegistered(details));\r\n    }\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps, mapActionToProps)(MoreInfo);\r\n","/home/vedant/Projects/xenia-dashboard/src/Components/Contact/Contact.js",["216","217"],"/home/vedant/Projects/xenia-dashboard/src/Components/Cart/MainCart.js",[],["218","219"],"/home/vedant/Projects/xenia-dashboard/src/Components/Sponsors/Sponsors.js",["220"],"/home/vedant/Projects/xenia-dashboard/src/Components/AboutUs/About.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/WebTeam/WebTeam.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Footer/Footer.js",[],"/home/vedant/Projects/xenia-dashboard/src/Components/Profile/Profile.js",["221"],"import React, {useCallback } from \"react\";\r\nimport \"./Profile.css\";\r\nimport {loggedOut as notifyLoggedOut} from '../Notifications/Notification';\r\n\r\nimport { connect } from \"react-redux\";\r\nimport { loggedOut  } from \"../../Store/Actions\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nconst MyProf = (props) => {\r\n  // const showDetails = useCallback((e) => {\r\n  //   const cont = document.querySelector(\".registeredEventInfo\");\r\n\r\n  //   cont.innerHTML = `<div class=\"slotDetails\">\r\n  //               <h1 class=\"registeredEventInfoName\">${e.target.textConten